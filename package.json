{
  "name": "traffic-tracker",
  "version": "1.0.0",
  "description": "Cloudflare Worker that collects travel time estimates between Brooklyn and Westport, CT",
  "main": "src/index.ts",
  "type": "module",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create traffic-tracker",
    "db:migrate": "wrangler d1 execute traffic-tracker --file=schema.sql --remote",
    "db:migrate:multi-route": "wrangler d1 execute traffic-tracker --remote --file=migrations/001-multi-route.sql",
    "routes:push": "node scripts/push-routes.js"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "@types/js-yaml": "^4.0.9",
    "js-yaml": "^4.1.1",
    "typescript": "^5.7.2",
    "wrangler": "^4"
  }
}
