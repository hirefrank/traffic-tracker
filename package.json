{
  "name": "traffic-tracker",
  "version": "1.0.0",
  "description": "Cloudflare Worker that collects travel time estimates between Brooklyn and Westport, CT",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create traffic-tracker",
    "db:migrate": "wrangler d1 execute traffic-tracker --file=schema.sql --remote"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.7.2",
    "wrangler": "^4"
  }
}
